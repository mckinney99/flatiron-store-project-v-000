<p>
<% @categories.each do |category| %>
  <%= link_to category.title, category_path(category) %> |
<% end %>
</p>


<% @items.each do |item| %>
  <p><%= item.title %> | <%= number_to_currency(item.price/100) %> | <%= button_to 'Add to Cart', line_items_path(item_id: item.id) if logged_in? %></p>
<% end %>
